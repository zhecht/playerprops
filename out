  this.currentSite = 'default';
  this.currentSort;
  this.updatedPlayerArray = [];
  this.minSalary = 0;
  this.maxSalary = 13000;
  this.salaryStep = 100;
  this.minAb = 0;
  this.maxAb = 100;
  this.minSo = 0;
  this.maxSo = 30;
  this.minAVG = 0;
  this.maxAVG = 1;
  this.minOBP = 0;
  this.maxOBP = 1;
  this.minSLG = 0;
  this.maxSLG = 4;
}

Model.prototype = {
  setProperSiteForView: function(){
    if(this.currentSite=='default'){
      return 'fd';
    }else{
      return this.currentSite;
    }
  },
  updateSalariesForSites: function(site){
    if(site=='fd'||site=='dk'||site=='default'){
      this.maxSalary = 13000;
      this.salaryStep = 100;
    }else if(site=='ya'){
      this.maxSalary = 200;
      this.salaryStep = 5;
    }
  },
  determineCurrentSort: function(type){
    if(type=='batter'&&this.currentSite=='default'){
      this.currentSort = 'batter';
      return this.alph_sort;
    }else if(type=='batter'&&this.currentSite!='default'){
      this.currentSort = 'batter_salary_'+this.currentSite;
      return this.int_sort;
    }else if(type=='pitcher'&&this.currentSite=='default'){
      this.currentSort = 'pitcher';
      return this.alph_sort;
    }else if(type=='pitcher'&&this.currentSite!='default'){
      this.currentSort = 'pitcher_salary_'+this.currentSite;
      return this.int_sort;
    }else if(type=='batting_avg'||type=='obp'||type=='slugging_pct'){
      this.currentSort = type;
      return this.float_sort;
    }else{
      this.currentSort = type;
      return this.int_sort;
    }
  },
  alph_sort: function(key,a,b){
    if((a[key]) < (b[key])) return -1;
    if((a[key]) > (b[key])) return 1;
    return 0;
  },

  int_sort: function(key,a,b){
    if(parseInt(a[key]) < parseInt(b[key])) return -1;
    if(parseInt(a[key]) > parseInt(b[key])) return 1;
    return 0;
  },

  float_sort: function(key,a,b){
    if(parseFloat(a[key]) < parseFloat(b[key])) return -1;
    if(parseFloat(a[key]) > parseFloat(b[key])) return 1;
    return 0;
  },

  default_sort: function(a,b){
    if(parseInt(a['h']) < parseInt(b['h'])) return -1;
    if(parseInt(a['h']) > parseInt(b['h'])) return 1;
    return 0;
  },
  masterFilter: function(search, position){
    var self = this;
    self.updatedPlayerArray = [];
    $.each(self.playerArray, function(key,value){
      if(self.checkSearchText(search,value.batter)&&self.checkPosition(position,value[self.setProperSiteForView()+'_pos_batter'])&&self.checkSliderConstraints(value)){
        self.updatedPlayerArray.push(value);
      }
    })
    return self.updatedPlayerArray;
  },
  checkSearchText: function(search, name){
    if(name.toLowerCase().indexOf(search.toLowerCase())>=0){
      return true;
    }else{
      return false;
    }
  },

  checkPosition: function(tab,pos){
    if(tab=='All'){
      return true;
    }else if(pos==tab){
      return true;
    }else{
      return false;
    }
  },
  checkSliderConstraints: function(player){
    var self = this;
    if(self.checkSalary(player['batter_salary_'+self.setProperSiteForView()], player['pitcher_salary_'+self.setProperSiteForView()])&&self.checkAb(player['ab'])&&self.checkSo(player['so'])&&self.checkAVG(player['batting_avg'])&&self.checkOBP(player['obp'])&&self.checkSLG(player['slugging_pct'])){
      return true;
    }else{
      return false;
    }
  },
  checkSalary: function(batter, pitcher){
    if(parseInt(batter)>=parseInt(this.minSalary)&&parseInt(batter)<=parseInt(this.maxSalary)&&batter!="N/A"){
      return true;
    }else if(batter=='N/A'||pitcher=='N/A'){
      return true;
    }else{
      return false;
    }
  },
  checkAb: function(ab){
    if(parseInt(ab)>=parseInt(this.minAb)&&parseInt(ab)<=parseInt(this.maxAb)){
      return true;
    }else{
      return false;
    }
  },
  checkSo: function(so){
    if(parseInt(so)>=parseInt(this.minSo)&&parseInt(so)<=parseInt(this.maxSo)){
      return true;
    }else{
      return false;
    }
  },
  checkAVG: function(avg){
    if(parseFloat(avg)>=parseFloat(this.minAVG)&&parseFloat(avg)<=parseFloat(this.maxAVG)){
      return true;
    }else{
      return false;
    }
  },
  checkOBP: function(obp){
    if(parseFloat(obp)>=parseFloat(this.minOBP)&&parseFloat(obp)<=parseFloat(this.maxOBP)){
      return true;
    }else{
      return false;
    }
  },
  checkSLG: function(slg){
    if(parseFloat(slg)>=parseFloat(this.minSLG)&&parseFloat(slg)<=parseFloat(this.maxSLG)){
      return true;
    }else{
      return false;
    }
  }
}

function View(){

}

View.prototype = {
  appendPlayers: function(players, site){
    $('.stat-table').empty();
    var self = this;
    $.each(players, function(key,value){
      var playerString = '<tr class="">'+
                  '<td class="stat-name-width"><img src="../../style/assets/img/mlb/team-logos/'+value.batter_team.toLowerCase().replace(" ","")+'.png" height="20"><span class="batter-name"> '+value.batter+'</span><small class="text-muted batter-hand">  ('+value.bats+') </small><div class="fantasy-blue inline"><small class="text-muted">  '+value[site+'_pos_batter']+'</small> <span class="fantasy-data">$'+self.formatSalary(value['batter_salary_'+site])+'</span></div> <span class="starting">'+value.status+'</span></td>'+
                  '<td class="stat-name-width"><img src="../../style/assets/img/mlb/team-logos/'+value.pitcher_team.toLowerCase().replace(" ","")+'.png" height="20"><span class="pitcher-name"> '+value.pitcher+'</span><div class="fantasy-blue inline" ><small class="text-muted pitcher-hand">  ('+value.p_throws+')</small> <span class="fantasy-data">$'+self.formatSalary(value['pitcher_salary_'+site])+'</span></div></td>'+
                  '<td class="stat-stat-width">'+value.pa+'</td>'+
                  '<td class="stat-stat-width">'+value.ab+'</td>'+
                  '<td class="stat-stat-width">'+value.h+'</td>'+
                  '<td class="stat-stat-width">'+value.singles+'</td>'+
                  '<td class="stat-stat-width">'+value.doubles+'</td>'+
                  '<td class="stat-stat-width">'+value.triples+'</td>'+
                  '<td class="stat-stat-width">'+value.hr+'</td>'+
                  '<td class="stat-stat-width">'+value.bb+'</td>'+
                  '<td class="stat-stat-width">'+value.so+'</td>'+
                  '<td class="stat-stat-width-2">'+value.batting_avg+'</td>'+
                  '<td class="stat-stat-width-2">'+value.obp+'</td>'+
                  '<td class="stat-stat-width-2">'+value.slugging_pct+'</td></tr>'
                  // '<td class="stat-stat-width fantasy-data">'+value['fpts_'+site+'_ppa']+'</td></tr>';
    $('.stat-table').append(playerString);
    })
  self.hideShowFantasyData($('.activeSite').first().text());
  },
  formatSalary: function(x){
    return x.toString().replace(/\B(?=(\d{3})+(?!\d))/g, ",");
  },
  updatePositionTab: function(tab){
    $('.nav-tabs-dfs').children().removeClass('active-dfs');
    $(tab).parent().addClass('active-dfs');
  },
  getSearchText: function(){
    return $('#search_text').val();
  },
  getCurrentTab: function(){
    return $('.active-dfs').children().text();
  },
  hideShowFantasyData: function(site){
    if(site=='Default'){
      $('.fantasy-data').hide();
      $('.fantasy-sliders').hide();
    }else{
      $('.fantasy-data').show();
      $('.fantasy-sliders').show();
      $('.fantasy-data').css('display','inline');
    }
  },
  updateSiteButton: function(tab){
    $('.btn-group').children().children().removeClass('active activeSite');
    $(tab).addClass('active activeSite');
  },
}

function Controller(){
  this.view = new View();
  this.model = new Model();
}

Controller.prototype = {
  loadInitalPlayers: function(){
    this.view.appendPlayers(this.model.playerArray, this.model.setProperSiteForView());
  },
  positionTabListener: function(){
    var self = this;
    $('.positions').click(function(){
      self.view.updatePositionTab($(this));
      self.view.appendPlayers(self.model.masterFilter(self.view.getSearchText(),self.view.getCurrentTab()),self.model.setProperSiteForView());
    })
  },
  searchListener: function(){
    var self = this;
    $('#search_text').keyup(function(){
      self.view.appendPlayers(self.model.masterFilter(self.view.getSearchText(),self.view.getCurrentTab()),self.model.setProperSiteForView());
    })
  },
  siteListener: function(){
    var self = this;
    $('.fantasySite').click(function(e){
      e.preventDefault();
      self.model.updateSalariesForSites($(this).attr('site'));
      self.view.updateSiteButton($(this));
      self.model.currentSite = $(this).attr('site');
      $("#slider")[0].destroy();
      self.buildSalarySlider(self.model.maxSalary, self.model.salaryStep);
      self.view.appendPlayers(self.model.masterFilter(self.view.getSearchText(),self.view.getCurrentTab()),self.model.setProperSiteForView());
    })
  },
  buildSliders: function(){
    var self = this;
    $("#slider2").noUiSlider({
      animate: true,
      start: [0, 100],
      step: 1,
      connect: true,
      format: wNumb({
        decimals: 0
      }),
      range: {
        'min': 0,
        'max': 100
      }
    });
    $('#slider2').Link('lower').to($('#slider2-snap-value-lower'));
    $('#slider2').Link('upper').to($('#slider2-snap-value-upper'));
    $('#slider2').change(function(){
      var sliderValues = ($('#slider2').val());
      self.model.minAb = sliderValues[0];
      self.model.maxAb = sliderValues[1];
      self.view.appendPlayers(self.model.masterFilter(self.view.getSearchText(),self.view.getCurrentTab()),self.model.setProperSiteForView());
    });

    $("#slider3").noUiSlider({
      animate: true,
      start: [.000, 1.000],
      step: .001,
      connect: true,
      format: wNumb({
        decimals: 3
      }),
      range: {
        'min': .000,
        'max': 1.000
      }
    });
    $('#slider3').Link('lower').to($('#slider3-snap-value-lower'));
    $('#slider3').Link('upper').to($('#slider3-snap-value-upper'));
    $('#slider3').change(function(){
      var sliderValues = ($('#slider3').val());
      self.model.minAVG = parseFloat(sliderValues[0]);
      self.model.maxAVG = parseFloat(sliderValues[1]);
      self.view.appendPlayers(self.model.masterFilter(self.view.getSearchText(),self.view.getCurrentTab()),self.model.setProperSiteForView());
    });

    $("#slider4").noUiSlider({
      animate: true,
      start: [0, 20],
      step: 1,
      connect: true,
      format: wNumb({
        decimals: 0
      }),
      range: {
        'min': 0,
        'max': 20
      }
    });
    $('#slider4').Link('lower').to($('#slider4-snap-value-lower'));
    $('#slider4').Link('upper').to($('#slider4-snap-value-upper'));
    $('#slider4').change(function(){
      var sliderValues = ($('#slider4').val());
      self.model.minSo = sliderValues[0];
      self.model.maxSo = sliderValues[1];
      self.view.appendPlayers(self.model.masterFilter(self.view.getSearchText(),self.view.getCurrentTab()),self.model.setProperSiteForView());
    });

    $("#slider5").noUiSlider({
      animate: true,
      start: [.000, 1.000],
      step: .001,
      connect: true,
      format: wNumb({
        decimals: 3
      }),
      range: {
        'min': .000,
        'max': 1.000
      }
    });
    $('#slider5').Link('lower').to($('#slider5-snap-value-lower'));
    $('#slider5').Link('upper').to($('#slider5-snap-value-upper'));
    $('#slider5').change(function(){
      var sliderValues = ($('#slider5').val());
      self.model.minOBP = sliderValues[0];
      self.model.maxOBP = sliderValues[1];
      self.view.appendPlayers(self.model.masterFilter(self.view.getSearchText(),self.view.getCurrentTab()),self.model.setProperSiteForView());
    });

    $("#slider6").noUiSlider({
      animate: true,
      start: [.000, 4.000],
      step: .001,
      connect: true,
      format: wNumb({
        decimals: 3
      }),
      range: {
        'min': .000,
        'max': 4.000
      }
    });
    $('#slider6').Link('lower').to($('#slider6-snap-value-lower'));
    $('#slider6').Link('upper').to($('#slider6-snap-value-upper'));
    $('#slider6').change(function(){
      var sliderValues = ($('#slider6').val());
      self.model.minSLG = sliderValues[0];
      self.model.maxSLG = sliderValues[1];
      self.view.appendPlayers(self.model.masterFilter(self.view.getSearchText(),self.view.getCurrentTab()),self.model.setProperSiteForView());
    });
  },
  buildSalarySlider: function(max,step){
    var self = this;
    $("#slider").noUiSlider({
      animate: true,
      start: [0, max],
      step: step,
      connect: true,
      format: wNumb({
        decimals: 0,
        prefix: '$'
      }),
      range: {
        'min': 0,
        'max': max
      }
    });
    $('#slider').Link('lower').to($('#slider-snap-value-lower'));
    $('#slider').Link('upper').to($('#slider-snap-value-upper'));
    $('#slider').change(function(){
      var sliderValues = ($('#slider').val());
      self.model.minSalary = sliderValues[0].replace('$','');
      self.model.maxSalary = sliderValues[1].replace('$','');
      self.view.appendPlayers(self.model.masterFilter(self.view.getSearchText(),self.view.getCurrentTab()),self.model.setProperSiteForView());
    });
  },

  sortListener: function(){
    var self = this;
    $('.stat-sort').click(function(){
      var sortFunction = self.model.determineCurrentSort($(this).attr('type'));
      if ($(this).hasClass('sorted')){
        $(this).removeClass('sorted');
        $(this).addClass('reverseSorted');
        var sortBy = 'reverse';
      } else if ($(this).hasClass('reverseSorted')){
        $(this).removeClass('reverseSorted');
        var sortBy = 'unsort';
      } else {
        $('.sort-columns').children().children().removeClass('sorted');
        $('.sort-columns').children().children().removeClass('reverseSorted');
        $(this).addClass('sorted');
        var sortBy = 'sort';
      }
      self.performSort(sortFunction, sortBy);
    })
  },


  performSort: function(sortFunction, sortBy){
    var self = this;
    if (sortBy == 'sort'){
      self.model.playerArray.sort(sortFunction.bind(null, self.model.currentSort));
    } else if (sortBy == 'reverse'){
      self.model.playerArray.sort(sortFunction.bind(null, self.model.currentSort)).reverse();
    } else if (sortBy == 'unsort'){
      self.model.playerArray.sort(self.model.default_sort).reverse();
    }
    self.view.appendPlayers(self.model.masterFilter(self.view.getSearchText(),self.view.getCurrentTab()),self.model.setProperSiteForView());
  },
}

$(document).ready(function(){
  var controller = new Controller();
  controller.loadInitalPlayers();
  controller.positionTabListener();
  controller.searchListener();
  controller.siteListener();
  controller.buildSliders();
  controller.buildSalarySlider(12000,100);
  controller.sortListener();
})

</script>
