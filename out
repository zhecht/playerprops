         </script>
<script type="274ba398625ee043d485c03e-text/javascript">
        $(document).ready(function(){
          $('.non-fantasySite').click(function(e){
            e.preventDefault();
            $('.btn-group').children().children().removeClass('active activeSite');
            $('.allPos').addClass('active-dfs active-non-fantasy');
            $(this).addClass('active activeSite');
            $('.fantasy-data').hide();
            $('.non-fantasy-data').show();
          })
        })

       </script>
<script type="274ba398625ee043d485c03e-text/javascript">
           $(function() {
            var $table = $('#stat-table').tablesorter({
                widgets: ["columns", "filter"],
                widgetOptions : {

                    filter_columnFilters: false,
                    filter_reset: '.reset'
                }

            });
            $.tablesorter.filter.bindSearch( $table, $('.search') );

            });
         </script>
