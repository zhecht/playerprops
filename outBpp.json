<script>
	$(document).ready(function() {
		var optionsBatter = {
			url: function(phrase) {
				return "fetchAutocompletePlayer.php?searchterm=" + phrase + "&format=json";
			},

			getValue: "name",
			placeholder: "Start typing a player...",

			list: {

				onSelectItemEvent: function() {
					var value = $("#batter").getSelectedItemData();
					$batter = value.code
					$playerType = value.playerType
				}
			},
			theme: "round",

			template: {
				type: "custom",
				method: function(value, item) {
					return "<p style='font-size:24px; padding:0px; margin:0px;'><img src='" + item.icon + "' style='width:50px;height:24px;' />" + value + "</p>";
				}
			},
		delay: 500 // Delay in milliseconds
		};

		$("#batter").easyAutocomplete(optionsBatter);
		$('#goButton').click(function()
		{
				window.location.href = $playerType + '.php?PlayerId=' + $batter;
		});
		$('#modelPrediction').DataTable({
			"ordering": false,
			"paging": false,
			"dom": '<"wrapper"t>'
		});
	});

</script>
