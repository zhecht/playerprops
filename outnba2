            <link href="/frontend/assets/css/ckeditor.css?v=2" rel="stylesheet" type="text/css">
        <script type="text/javascript" src="/frontend/assets/js/lineup-alerts.min.js?v=7"></script><script defer src="/frontend/assets/js/nba-lineups.min.js?v=4"></script><script src="/frontend/assets/js/deposit-offers.js?v=1"></script>    <script src="/frontend/assets/js/rotowire.min.js?v=197"></script>
        <script>
        // Google Analytics
        const isSubscriber = '1'
        if(!isSubscriber){
         // Add Lineup Alerts
         const lineupAlertsButtonElements = [...$('.lineup-alerts-bar__button.lineup-alerts-view')]
            if(lineupAlertsButtonElements.length > 0){
                lineupAlertsButtonElements.forEach(button => {
                    $(button).addClass('ga-event-nba_lineups_alert')
                })
            }
        }

        // Picks Button
        const picksButtons = [...$('.picks-button')]
        if(picksButtons.length > 0){
            picksButtons.forEach(button => {
                $(button).addClass('ga-event-lineups_picks_tool_nba')
            })
        }

        // Projected Minutes and On/Off Court Stats

        // Home team elements only are paywalled
        let homeListElements = [...$('.lineup__list.is-home')]
        let visitListElements = [...$('.lineup__list.is-visit')]
        
        if(homeListElements.length > 0){
            homeListElements.forEach(element => {
                const projectedMinutes = $(element).find('button.see-proj-minutes')
                if(projectedMinutes.length > 0){
                    // Add GA Event classes to all See Projected Minutes buttons
                    if(isSubscriber){
                        $(projectedMinutes).addClass('ga-event-projected_minutes')
                    }else{
                        $(projectedMinutes).addClass('ga-event-projected_minutes_up')
                    }

                    $(projectedMinutes).attr('data-section', 'nba')

                }
                const courtOnOffButton = $(element).find('button.see-court-on-off')
                if(courtOnOffButton.length > 0){
                    
                    // Add GA Event classes to all Court On/Off buttons
                    if(isSubscriber){
                        $(courtOnOffButton).addClass('ga-event-on_off_court_stats')
                    }else{
                        $(courtOnOffButton).addClass('ga-event-on_off_court_stats_up')
                    }

                    $(courtOnOffButton).attr('data-section', 'nba')

                }
            })
        }
        if(visitListElements.length > 0){
            visitListElements.forEach(element => {
                const projectedMinutes = $(element).find('button.see-proj-minutes')
                if(projectedMinutes.length > 0){
                    // Add GA Event classes to all Home Run Odds buttons
                    $(projectedMinutes).addClass('ga-event-projected_minutes')
                    $(projectedMinutes).attr('data-section', 'nba')

                }
                const courtOnOffButton = $(element).find('button.see-court-on-off')
                if(courtOnOffButton.length > 0){
                    
                    // Add GA Event classes to all Starting Pitcher buttons
                    $(courtOnOffButton).addClass('ga-event-on_off_court_stats')
                    $(courtOnOffButton).attr('data-section', 'nba')

                }
            })
        }
        

    </script>
