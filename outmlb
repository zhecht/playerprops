			<th style="background-color:#e33724;text-align:center; color:#ededed">FN</th>
			<th style="background-color:#fa3628;text-align:center; color:#FFFFFF">ON</th>
			<th style="background-color:#138042;text-align:center; color:#e8d310">B3</th>
			<th style="background-color:#216aad;text-align:center; color:#ffffff">SH</th>
