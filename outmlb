<script>
// Function to save preferences and reload the page with new parameters
function savePreferences() {
    var currentUrl = new URL(window.location.href);
    for (var i = 1; i <= 6; i++) {
        var book = document.getElementById('bookSelect' + i).value;
        currentUrl.searchParams.set('book' + i, book);
    }
    window.location.href = currentUrl.href;
}

function updateBetSideAndReload(betSide) {
    var currentUrl = new URL(window.location.href);
    currentUrl.searchParams.set('BetSide', betSide);
    window.location.href = currentUrl.href;
}

// Attach event listeners for BetSide buttons
document.getElementById('overButton').addEventListener('click', function() {
    updateBetSideAndReload(1); // Over button sets BetSide to 1
});

document.getElementById('underButton').addEventListener('click', function() {
    updateBetSideAndReload(-1); // Under button sets BetSide to -1
});

// Initialize the button states based on the current URL parameter
var currentBetSide = new URLSearchParams(window.location.search).get('BetSide');
if (currentBetSide === '-1') {
    document.getElementById('overButton').classList.remove('active');
    document.getElementById('underButton').classList.add('active');
} else {
    document.getElementById('overButton').classList.add('active');
    document.getElementById('underButton').classList.remove('active');
}
</script>

