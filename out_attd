            document.getElementById("CheckBoxBoost").addEventListener("change", function () {
        
                var _checkbox_str = "CheckBoxBoost";
                var _span_str = "spanBoost";
                var _checkbox = document.getElementById(_checkbox_str);
                var _span = document.getElementById(_span_str);

                if (_checkbox.checked == true && _span.style.display == 'none') {

                    _span.style.display = 'inline';
                }
                else if (_checkbox.checked == false && _span.style.display == 'inline') {
                    _span.style.display = 'none';
                }
            });

            document.getElementById("CheckBoxCorrelation").addEventListener("change", function () {
                var _checkbox_str = "CheckBoxCorrelation";
                var _span_str = "spanCorrelation";
                var _checkbox = document.getElementById(_checkbox_str);
                var _span = document.getElementById(_span_str);

                if (_checkbox.checked == true && _span.style.display == 'none') {

                    _span.style.display = 'inline';
                }
                else if (_checkbox.checked == false && _span.style.display == 'inline') {
                    _span.style.display = 'none';
                }
            });

            document.getElementById("CheckBoxDailyFantasy").addEventListener("change", function () {
                var _checkbox_str = "CheckBoxDailyFantasy";
                var _span_str = "spanDailyFantasy";
                var _checkbox = document.getElementById(_checkbox_str);
                var _span = document.getElementById(_span_str);

                if (_checkbox.checked == true && _span.style.display == 'none') {

                    _span.style.display = 'inline';
                }
                else if (_checkbox.checked == false && _span.style.display == 'inline') {
                    _span.style.display = 'none';
                }
            });

            document.getElementById("DropDownListDailyFantasy").addEventListener("change", function () {
                var _dropdown_str = "DropDownListDailyFantasy";
                var _textbox_str = "TextBoxFinalOdds";
                var _dropdown = document.getElementById(_dropdown_str);
                var _textbox = document.getElementById(_textbox_str);

                _textbox.value = _dropdown.value;
            });

            function DynamicCheckBoxSection(_checkbox_str, _span_str)
            {
                var _checkbox = document.getElementById(_checkbox_str);
                var _span = document.getElementById(_span_str);

                if (_checkbox.checked == true && _span.style.display == 'none') {

                    _span.style.display = 'inline';
                }
                else if (_checkbox.checked == false && _span.style.display == 'inline') {
                    _span.style.display = 'none';
                }
            }

            function DynamicDropDownSelection(_dropdown_str, _textbox_str)
            {

                var _dropdown = document.getElementById(_dropdown_str);
                var _textbox = document.getElementById(_textbox_str);

                _textbox.value = _dropdown.value;
            }

            DynamicCheckBoxSection("CheckBoxCorrelation", "spanCorrelation");

            DynamicCheckBoxSection("CheckBoxBoost", "spanBoost");

            DynamicCheckBoxSection("CheckBoxDailyFantasy", "spanDailyFantasy");

            //DynamicDropDownSelection("DropDownListDailyFantasy", "TextBoxFinalOdds");

            function CopyToClipboard(id) {
                // Get the text field
                var copyText = document.getElementById(id);
                /*
                // Select the text field
                copyText.select();
                copyText.setSelectionRange(0, 99999); // For mobile devices

                // Copy the text inside the text field
                try {
                    navigator.clipboard.writeText(copyText.value);
                }
                catch
                {
                    document.execCommand('copy');
                }
                */
                // Alert the copied text
                //alert("Copied the text: " + copyText.value);

                //New, to also work with iOS/Mac
                Clipboard.copy(copyText.value);
            }


            /*
             * iOS/Windows Clipboard Solution:
             * https://stackoverflow.com/questions/40147676/javascript-copy-to-clipboard-on-safari
             * Previous solution, did not work with iOS (only worked with Windows)
             */
            window.Clipboard = (function (window, document, navigator) {
                var textArea,
                    copy;

                function isOS() {
                    return navigator.userAgent.match(/ipad|iphone/i);
                }

                function createTextArea(text) {
                    textArea = document.createElement('textArea');
                    textArea.value = text;
                    document.body.appendChild(textArea);
                }

                function selectText() {
                    var range,
                        selection;

                    if (isOS()) {
                        range = document.createRange();
                        range.selectNodeContents(textArea);
                        selection = window.getSelection();
                        selection.removeAllRanges();
                        selection.addRange(range);
                        textArea.setSelectionRange(0, 999999);
                    } else {
                        textArea.select();
                    }
                }

                function copyToClipboard() {
                    document.execCommand('copy');
                    document.body.removeChild(textArea);
                }

                copy = function (text) {
                    createTextArea(text);
                    selectText();
                    copyToClipboard();
                };

                return {
                    copy: copy
                };
            })(window, document, navigator);
            /*
             * End Clipboard
             */ 

        </script>


