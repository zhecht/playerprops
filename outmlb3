<div class="filter switcher" data-controls="#switcher_players_standard_batting">

<div class=" current">
	<a class="sr_preset" data-hide="#all_players_standard_batting .section_heading,  #all_players_standard_batting .footer,  #all_players_standard_batting .topscroll_div" data-show=".assoc_players_standard_batting, #tfooter_players_standard_batting">Regular Season</a>
